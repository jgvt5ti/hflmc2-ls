%HES
S =v ∀n.∀ls. (nlen n ls) \/ G ls A (n + 1).
A n =v true.
G ls k u =v u > 0 /\ ∀hd.∀tl. (ls <>l [] \/ k 0) /\
(ls <>l (hd :: tl) \/
  (hd <> 0 \/ G tl A (u - 1)) /\
  (hd <> 1 \/
      ∀hd2.∀tl2.
      (tl <>l [] \/ k 1) /\ 
      ((tl <>l (hd2 :: tl2) \/
      (hd2 <> 0 \/ G tl2 A (u - 1)) /\
      (hd2 <> 1 \/ G tl2 A (u - 1))
)))).